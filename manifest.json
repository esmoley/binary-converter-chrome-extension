{
  "name": "Дешифратор бинарного текста",
  "description": "Находит на странице бинарный текст и расшифровывает его",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "/images/binary_converter_extension_icon16.png",
      "32": "/images/binary_converter_extension_icon32.png",
      "48": "/images/binary_converter_extension_icon48.png",
      "128": "/images/binary_converter_extension_icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/binary_converter_extension_icon16.png",
    "32": "/images/binary_converter_extension_icon32.png",
    "48": "/images/binary_converter_extension_icon48.png",
    "128": "/images/binary_converter_extension_icon128.png"
  },
  "permissions": ["activeTab", "scripting"]
}